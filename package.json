{
  "name": "library-app",
  "version": "0.0.1",
  "description": "book management",
  "main": "dist/server.js",
  "scripts": {
    "watch": "webpack -d --watch",
    "lint": "eslint src",
    "build:dev": "webpack -d",
    "run-server:dev": "nodemon dist/server.js -e js --watch dist/server.js",
    "test": "jest --runInBand --forceExit",
    "coverage": "jest --coverage --runInBand --forceExit --testResultsProcessor @athena/app-fabric-coverage-jest --coverageReporters text",
    "start:dev": "npm-run-all --parallel watch run-server:dev"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "devDependencies": {
    "@athena/eslint-config-app-fabric": "^1.5.0",
    "babel-cli": "^6.13.2",
    "babel-core": "^6.21.0",
    "babel-jest": "^9.0.3",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-runtime": "^6.20.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "nodemon": "^1.3.8",
    "npm-run-all": "^4.1.1",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "regenerator-runtime": "^0.11.1",
    "resolve-url-loader": "^2.2.0",
    "sqlite3": "^4.0.0",
    "supertest": "^3.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@athena/app-fabric-coverage-jest": "^1.0.0",
    "@athena/app-fabric-logger": "^2.0.0",
    "@athena/forge": "^0.8.0",
    "axios": "^0.17.1",
    "body-parser": "^1.18.3",
    "express": "^4.14.0",
    "http-status-codes": "^1.3.0",
    "https": "^1.0.0",
    "pg": "^7.4.3",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-icons": "^2.2.7",
    "sequelize": "^4.37.1",
    "sinon": "^4.5.0",
    "validate.js": "^0.11.1",
    "validator": "^10.2.0"
  }
}
